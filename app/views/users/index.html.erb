<%= flash[:created] if flash[:created] %>
<%= flash[:deleted] if flash[:deleted] %>
<%= flash[:updated] if flash[:updated] %>

<% @user.each do |user| %>
	<p><%= user.fname %></p>	
	<p><%= user.lname %></p>
	<p><%= user.username %></p>

	<%= link_to "Edit", edit_user_path(user.id) %>
 	<%= link_to "Delete", user, data: { confirm: "Are you sure?" }, method: :delete %>
<% end %>